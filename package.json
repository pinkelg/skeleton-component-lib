{
  "name": "skeleton-component-lib",
  "version": "1.0.0",
  "description": "Skeleton app for creating react component library using storybook and rollup",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production BABEL_ENV=production rollup -c",
    "test": "jest",
    "test:coverage": "jest --coverage --colors",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint . --ext src/**/*.js,.jsx,.ts,.tsx",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinkelg/skeleton-component-lib.git"
  },
  "keywords": [],
  "author": "Pinkel Gurung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinkelg/skeleton-component-lib/issues"
  },
  "homepage": "https://github.com/pinkelg/skeleton-component-lib#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^6.5.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-jest": "^29.3.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-pnp-resolver": "^1.2.3",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "react-app-polyfill": "^3.0.0",
    "regenerator-runtime": "^0.13.11",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
